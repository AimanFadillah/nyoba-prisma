model Barang {
  id String @id @default(uuid())
  nama String
  jumlah Int
  user User @relation(fields: [user_id],references: [id],onDelete: Cascade)
  user_id String
}

model User {
  id String @id @default(uuid())
  nama String
  barangs Barang[]
}